{
  "name": "boily",
  "version": "0.0.12",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",	  
    "build": "node config/rollup.config.js",
    "browser": "webpack-dev-server --quiet --config config/webpack.test.conf.js",
    "test:server": "mocha --opts config/mocha.opts",
    "test:browser": "karma start config/karma.conf.js --single-run",
    "test": "npm run test:server & npm run test:browser",
    "eslint:source": "eslint ./src/**/*.js",
    "eslint:fix": "eslint --fix ./src/**/*.js",
    "eslint:shared": "eslint ./test/shared/**/*.js",
    "eslint:server": "eslint ./test/server/**/*.js",
    "eslint:browser": "eslint ./test/browser/**/*.js",
    "watch:server": "npm run test:server -- --watch",
    "watch:browser": "./node_modules/.bin/karma start --auto-watch --no-single-run",
    "packages": "npm list --depth=0",
    "package:purge": "rm -rf node_modules",
    "package:reinstall": "npm run package:purge && npm install",
    "package:check": "./node_modules/.bin/ncu",
    "package:upgrade": "./node_modules/.bin/ncu -u",
    "package:prod": "./node_modules/.bin/ncu -u",
    "package:dev": "./node_modules/.bin/ncu -p -u",
    "preversion": "[[ -z $(git status --porcelain) ]] && npm run build && git add -A dist && git commit -m 'Build lib'",
    "postpublish": "git push origin master --follow-tags"
  },
  "pre-commit": [
    "test:browser",
    "test:server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kflash/boily"
  },
  "keywords": [],
  "files": [
    "dist",
    "src"
  ],
  "babel": {
	"ignore": [
      "packages/babel-cli/src/babel-plugin/templates"
    ]
  },
  "author": {
    "name": "KFlash",
    "email": "kflash123@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/kflash/boily/issues"
  },
  "devEngines": {
    "node": ">=4.1 <6",
    "npm": ">=3.1 <5"
  },
  "homepage": "https://github.com/kflash/boily",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-spec-reporter": "0.0.23",
	"karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "npm-check-updates": "^2.5.4",
    "phantomjs": "^1.9.19",
    "rollup": "^0.21.2",
    "rollup-plugin-babel": "^2.2.0",
    "rollup-plugin-npm": "^1.1.0",
    "rollup-plugin-replace": "^1.1.0",
    "uglify-js": "^2.6.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-core": "^6.3.17",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "karma-chrome-launcher": "^0.2.2",
    "pre-commit": "^1.1.2",
    "rimraf": "^2.4.4"
  }
}
